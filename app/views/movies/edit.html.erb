<div class="jumbotron">
<h1>Edit Movie</h1>


<ul class='nav nav-tabs'>
  <li><%= link_to "Back", @movie, :class => 'navbar-toggle collapsed' %></li>
  </ul>
</br>
<%= form_for @movie do |f| %>
  <div class="form-group">
  <p>
    <%= f.label "director" %><br>
    <%= f.text_field "director", :class => "form-control" %>
  </p>
  </div>
  <div class="form-group">
  <p>
    <%= f.label "title" %><br>
    <%= f.text_field "title", :class => "form-control" %>
  </p>
  </div>
  <div class="form-group">
  <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url", :class => "form-control" %>
  </p>
  </div>
  <div class="form-group">
  <p>
    <%= submit_tag "Save Changes", :class => 'btn btn-primary btn-block' %>
  </p>
  </div>
<% end %>
</div>
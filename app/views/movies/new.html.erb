<h1>Add a Movie</h1>
<%= link_to "Back to Movies", movies_url %>

<%= form_for @movie do |f| %>
  <p>
    <%= f.label "director" %><br>
    <%= f.text_field "director" %>
  </p>
  <p>
    <%= f.label "title" %><br>
    <%= f.text_field "title" %>
  </p>
  <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>